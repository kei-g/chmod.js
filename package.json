  "main": "index.js",
  "name": "@kei-g/chmod",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kei-g/chmod.js.git"
  },
  "scripts": {
    "build": "npm-run-all -p clean lint -p build:mkdir build:tsc -p build:terse:* -s build:chmod",
    "build:chmod": "ts-node src/bin/chmod.ts +x bin/chmod.js",
    "build:mkdir": "mkdirp bin lib",
    "build:terse:bin": "terser build/bin/chmod.js -c -m -o bin/chmod.js --toplevel",
    "build:terse:index": "terser build/index.js -c -m -o index.js --toplevel",
    "build:terse:lib": "terser build/lib/chmod.js -c -m -o lib/chmod.js --toplevel",
    "build:tsc": "tsc",
    "clean": "rimraf .nyc_output/ bin/ build/ coverage/ index.d.ts index.js lib/",
    "cover": "nyc --check-coverage -r html -r text _mocha",
    "lint": "eslint src/*.ts src/bin/*.ts src/lib/*.ts",
    "postpublish": "run-s clean",
    "prepublishOnly": "run-s build",
    "start": "ts-node src/bin/chmod.ts",
    "test": "run-p cover lint"
  },
  "types": "index.d.ts",
  "version": "1.0.2"
}
